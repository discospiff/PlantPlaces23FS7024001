{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "array",
  "items": {
    "$ref": "#/definitions/PlantElement"
  },
  "definitions": {
    "PlantElement": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "lat": {
          "type": "number",
          "minimum": -100,
          "maximum": 100
        },
        "lng": {
          "type": "number",
          "minimum": -150,
          "maximum": 150
          
        },
        "plant_id": {
          "type": "integer",
          "minimum": 0
        },
        "specimen_id": {
          "type": "integer",
          "minimum": 0
        },
        "common": {
          "type": "string"
        },
        "genus": {
          "type": "string",
          "minLength": 2,
          "maxLength": 65
        },
        "species": {
          "type": "string"
        },
        "address": {
          "type": "string"
        },
        "notes": {
          "type": "string"
        }
      },
      "required": [
        "genus",
        "lat",
        "lng",
        "plant_id",
        "specimen_id"
      ],
      "title": "PlantElement"
    }
  }
}
